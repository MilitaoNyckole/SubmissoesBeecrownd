#include <stdio.h>
 
int main() {
 
    int idade;
    
    scanf("%i", &idade);
    
    int ano =idade/365;
    
    int mes= (idade -(ano*365))/30;
    
    int dia = idade-((ano*365)+(mes*30));
    
    
    printf("%i ano(s)\n", ano);
    printf("%i mes(es)\n", mes);
    printf("%i dia(s)\n", dia);
    
    return 0;
}